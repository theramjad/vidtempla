# Google Sign-In OAuth (get from Google Cloud Console)
# Create OAuth 2.0 Client ID at: https://console.cloud.google.com/apis/credentials
# Configure authorized redirect URIs to include: http://localhost:3000/api/auth/callback
GOOGLE_CLIENT_ID=YOUR_GOOGLE_OAUTH_CLIENT_ID
GOOGLE_CLIENT_SECRET=YOUR_GOOGLE_OAUTH_CLIENT_SECRET

# YouTube OAuth (get from Google Cloud Console)
YOUTUBE_CLIENT_ID=YOUR_GOOGLE_OAUTH_CLIENT_ID
YOUTUBE_CLIENT_SECRET=YOUR_GOOGLE_OAUTH_CLIENT_SECRET
YOUTUBE_REDIRECT_URI=http://localhost:3000/api/auth/youtube/callback

# Encryption key for storing OAuth tokens (generate a random 32+ character string)
ENCRYPTION_KEY=YOUR_RANDOM_32_CHARACTER_ENCRYPTION_KEY

# RapidAPI (optional - for Twitter monitoring feature)
RAPID_API_KEY=YOUR_KEY_HERE

# Supabase
SUPABASE_SERVICE_ROLE_KEY=YOUR_KEY_HERE

# Inngest (required in production)
INNGEST_EVENT_KEY=YOUR_KEY_HERE
INNGEST_SIGNING_KEY=YOUR_KEY_HERE

# Google Gemini API
GEMINI_API_KEY="your-api-key-here"

# Stripe (Payments)
# Get these from your Stripe dashboard: https://dashboard.stripe.com/
STRIPE_SECRET_KEY=sk_test_YOUR_SECRET_KEY_HERE
STRIPE_WEBHOOK_SECRET=whsec_YOUR_WEBHOOK_SECRET_HERE
# Price IDs from Stripe (find these in Products section)
STRIPE_PRO_PRICE_ID=price_YOUR_PRO_PRICE_ID_HERE
STRIPE_BUSINESS_PRICE_ID=price_YOUR_BUSINESS_PRICE_ID_HERE

#
# Client
#

# App URL (used for redirects, webhooks, etc.)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Supabase
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_KEY_HERE
NEXT_PUBLIC_SUPABASE_URL=YOUR_KEY_HERE

# Stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_YOUR_PUBLISHABLE_KEY_HERE

# PostHog (optional - for analytics)
NEXT_PUBLIC_POSTHOG_KEY=phc_YOUR_POSTHOG_PROJECT_API_KEY_HERE
